{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "angular": false,
        "WEBUI_MODULE_NAME": true,
        "SERVICES_MODULE_NAME": true,
        "BLOCKS_MODULE_NAME": true,
        "Mocks": true,
        "Quantum": true,
        "Utilities": true,
        "expect": true,
        "fail": true,
        "describe": true,
        "fdescribe": true,
        "xdescribe": true,
        "browser": true,
        "element": true,
        "by": true,
        "afterEach": true,
        "beforeEach": true,
        "$httpBackend": true,
        "it": true,
        "fit": true,
        "xit": true,
        "inject": true,
        "jasmine": true,
        "spyOn": true
    },
    "rules": {
        "block-scoped-var": 2,
        "block-spacing": [1, "always"],
        "brace-style": 2,
        "camelcase": [
            2, {
                "properties": "always"
            }
        ],
        "comma-dangle": 1,
        "comma-spacing": [
            2, {
                "after": true,
                "before": false
            }
        ],
        "comma-style": [
            2,
            "last"
        ],
        "complexity": 1,
        "computed-property-spacing": [2, "never"],
        "consistent-this": [2, "vm", "self"],
        "curly": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "func-names": 2,
        "func-style": [2, "declaration"],
        "guard-for-in": 2,
        "id-length": [
            0, {
                "min": 2,
                "properties": "never",
                "exceptions": ["i", "j", "x", "y", "$", "_"]
            }
        ],
        "indent": 0,
        "init-declarations": [
            2,
            "never", {
                "ignoreForLoopInit": true
            }
        ],
        "key-spacing": [
            2, {
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "linebreak-style": 0,
        "max-depth": [2, 2],
        "max-len": [1, 160, 4],
        "max-nested-callbacks": [2, 3],
        "max-params": [2, 6],
        "max-statements": 0,
        "new-cap": [
            2, {
                "newIsCapExceptions": ["karmaServer"]
            }
        ],
        "newline-after-var": [2, "always"],
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-bitwise": ["error", {
            "allow": ["&"]
        }],
        "no-case-declarations": 2,
        "no-catch-shadow": 2,
        "no-cond-assign": 2,
        "no-console": 0,
        "no-constant-condition": 2,
        "no-continue": 2,
        "no-debugger": 1,
        "no-delete-var": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [2, "functions"],
        "no-extra-semi": 1,
        "no-func-assign": 2,
        "no-inline-comments": 2,
        "no-inner-declarations": [2, "both"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 0,
        "no-label-var": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-loop-func": 2,
        "no-magic-numbers": 0,
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
        "no-multiple-empty-lines": [
            2, {
                "max": 1,
                "maxEOF": 1
            }
        ],
        "no-native-reassign": 2,
        "no-negated-condition": 2,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-require": 2,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 2,
        "no-plusplus": 2,
        "no-process-env": 0,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-spaced-func": 2,
        "no-sparse-arrays": 2,
        "no-throw-literal": 2,
        "no-trailing-spaces": 2,
        "no-undef": 2,
        "no-undefined": 2,
        "no-undef-init": 2,
        "no-underscore-dangle": [
            2, {
                "allow": ["_id"]
            }
        ],
        "no-unexpected-multiline": 2,
        "no-unneeded-ternary": 2,
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": 1,
        "no-use-before-define": [2, "nofunc"],
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-void": 2,
        "no-warning-comments": 0,
        "no-with": 2,
        "object-curly-spacing": [2, "always"],
        "one-var": [0, "always"],
        "operator-assignment": [2, "always"],
        "operator-linebreak": [0],
        "padded-blocks": [
            2, "never"
        ],
        "quote-props": [
            2,
            "consistent-as-needed", {
                "keywords": true
            }
        ],
        "quotes": [2, "single"],
        "radix": [2, "always"],
        "semi": 2,
        "semi-spacing": [
            2, {
                "before": false,
                "after": true
            }
        ],
        "sort-vars": 2,
        "keyword-spacing": 2,
        "space-before-blocks": 2,
        "space-before-function-paren": [2, "never"],
        "space-infix-ops": 2,
        "space-in-parens": [2, "never"],
        "space-unary-ops": 2,
        "strict": [2, "function"],
        "use-isnan": 2,
        "valid-jsdoc": [
            2, {
                "requireReturn": false
            }
        ],
        "valid-typeof": 2,
        "vars-on-top": 2,
        "wrap-iife": [2, "inside"],
        "wrap-regex": 2,
        "yoda": 2
    }
}