doctype html
html
  head
    title= title
    
  body
    style.
      .addCar{
        font-size: 25px;
        text-align: center;
        margin-top: 230px;
      }
      .float {
        float: left;
      }
      .wrapper_body_button {
        margin-top: 10px;
        font-size: 11px;
        font-family: 'Open Sans', sans-serif;
